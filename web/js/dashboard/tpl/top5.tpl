<table class="table table-bordered">
    <tr class="forTr titleTop">
        <th colspan="6">${title}</th>
    </tr>
    <tr class="forTr top5Num">
        <th style="width:100px;">排名</th>
        <td>1</td>
        <td>2</td>
        <td>3</td>
        <td>4</td>
        <td>5</td>
    </tr>
    <tr class="top5Clothes">
        <th style="line-height: 6;">上衣</th>
        <td><img src="${up[0]}" width="100" height="100"/></td>
        <td><img src="${up[1]}" width="100" height="100"/></td>
        <td><img src="${up[2]}" width="100" height="100"/></td>
        <td><img src="${up[3]}" width="100" height="100"/></td>
        <td><img src="${up[4]}" width="100" height="100"/></td>
    </tr>
    <tr class="top5Clothes">
        <th style="line-height: 6;">裤子</th>
        <td><img src="${down[0]}" width="100" height="100"/></td>
        <td><img src="${down[1]}" width="100" height="100"/></td>
        <td><img src="${down[2]}" width="100" height="100"/></td>
        <td><img src="${down[3]}" width="100" height="100"/></td>
        <td><img src="${down[4]}" width="100" height="100"/></td>
    </tr>
    <tr class="top5Clothes">
        <th style="line-height: 6;">鞋</th>
        <td><img src="${foot[0]}" width="100" height="100"/></td>
        <td><img src="${foot[1]}" width="100" height="100"/></td>
        <td><img src="${foot[2]}" width="100" height="100"/></td>
        <td><img src="${foot[3]}" width="100" height="100"/></td>
        <td><img src="${foot[4]}" width="100" height="100"/></td>
    </tr>
    <tr class="forTr titleBottom">
        <th>选择数</th>
        <td>${num[0]}</td>
        <td>${num[1]}</td>
        <td>${num[2]}</td>
        <td>${num[3]}</td>
        <td>${num[4]}</td>
    </tr>
</table>
